static unsigned divide (unsigned long c)

int main(void)
{
	unsigned long i = 1;
	unsigned long c = 1;
	
	while (divide(n) < 500) 
	{
		n += ++i;
	}
	print(%lu\n", n);
	return 0;
}

unsigned divide (unsigned long c)
{
	unsigned ret = 1;
	unsigned long i;
	
	for (i = 2, i <= n; i++)
	{
		unsigned x = 0;
		while (n % i == 0)
		{
			x++;
			c /= i;
		}	
		ret *= x+1;
	}
	return ret;
}

